extends layout.pug

block content
    form(action=`/libraries/add/${albumId}` method="post")
        input(type='hidden' name='_csrf' value=csrfToken)
        label(for="libraryName") Name of Library
        input(type="text" name="libraryName")
        //- div(class='py-4')
        button(type='submit' class='') Add Library
        a(href='/libraries' class='') Cancel
